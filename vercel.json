
{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "/"
    },
    {
      "src": "/data",
      "dest": "/"
    },
    {
      "src": "/data-cleaning",
      "dest": "/"
    },
    {
      "src": "/predictive-models",
      "dest": "/"
    },
    {
      "src": "/dashboards",
      "dest": "/"
    },
    {
      "src": "/settings",
      "dest": "/"
    },
    {
      "src": "/hubspot/callback",
      "dest": "/"
    },
    {
      "src": "/zoho/callback",
      "dest": "/"
    },
    {
      "src": "/payment/callback",
      "dest": "/"
    }
  ],
  "github": {
    "silent": false
  },
  "build": {
    "env": {
      "VITE_LOVABLE_IDENTITY": "lovablebot.dev"
    }
  }
}
